{
  "name": "@toridoriv/codewars-javascript",
  "description": "My solutions for Codewars challenges in JavaScript.",
  "type": "module",
  "scripts": {
    "test": "node --experimental-vm-modules --no-warnings ./node_modules/.bin/jest --runInBand",
    "init": "scripty",
    "postinit": "prettier --config prettier.config.js -w ./solutions",
    "chmod": "chmod -R +x scripts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/toridoriv/codewars-javascript.git"
  },
  "keywords": [
    "toolkit",
    "utilities",
    "typescript"
  ],
  "author": {
    "name": "Tori Rodriguez",
    "email": "vrodriguezfe@icloud.com",
    "url": "https://github.com/toridoriv"
  },
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/toridoriv/codewars-javascript/issues"
  },
  "homepage": "https://github.com/toridoriv/codewars-javascript#readme",
  "devDependencies": {
    "@codewars/test-compat": "^1.1.0",
    "@homer0/prettier-plugin-jsdoc": "^8.0.0",
    "@types/mustache": "^4.2.5",
    "@types/node": "^18.19.3",
    "change-case": "^5.3.0",
    "cheerio": "^1.0.0-rc.12",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.7.0",
    "mustache": "^4.2.0",
    "prettier": "^3.1.1",
    "scripty": "^2.1.1",
    "typescript": "^5.3.3"
  },
  "eslintConfig": {
    "env": {
      "node": true,
      "browser": false,
      "commonjs": false
    },
    "extends": [
      "eslint:recommended",
      "plugin:prettier/recommended"
    ],
    "parserOptions": {
      "sourceType": "module",
      "ecmaVersion": "latest"
    },
    "plugins": [],
    "rules": {
      "no-unused-vars": "warn"
    }
  }
}
